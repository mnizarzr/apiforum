{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "consistent-return": "off"
    }
}
